  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: All debug Options",
      "type": "python",
      "request": "launch",
      "pythonPath": "${config:python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "env": {},
      "envFile": "${workspaceFolder}/.env",
      "args": ["arg1", "arg2"],
      "debugOptions": ["RedirectOutput"]
    }
  ]
}
